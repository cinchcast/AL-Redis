<?xml version="1.0" encoding="utf-8"?>
<!--
Web.config file for TestWebSite.

The settings that can be used in this file are documented at 
http://www.mono-project.com/Config_system.web and 
http://msdn2.microsoft.com/en-us/library/b5ysx397.aspx
-->
<configuration>
  <appSettings>
    <add key="RedisRetryTimeout" value="30000" />
    <add key="RedisNamespace" value="AMSTest" />
  </appSettings>
  <system.web>
    <compilation defaultLanguage="C#" debug="true" targetFramework="4.0">
      <assemblies />
    </compilation>
    <customErrors mode="RemoteOnly" />
    <authentication mode="None" />
    <authorization>
      <allow users="*" />
    </authorization>
    <httpModules>
      <remove name="Session" />
      <add name="Session" type="AngiesList.Redis.RedisSessionStateModule" />
    </httpModules>
    <httpHandlers />
    <trace enabled="true" localOnly="true" pageOutput="false" requestLimit="10" traceMode="SortByTime" />
    <sessionState stateConnectionString="tcp=10.211.55.2:6379" mode="Custom" cookieless="false" timeout="10">
      <providers></providers>
    </sessionState>
    <globalization requestEncoding="utf-8" responseEncoding="utf-8" />
    <pages controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID" />
  </system.web>
  <system.webServer>
    <modules>
      <remove name="Session" />
      <add name="Session" type="AngiesList.Redis.RedisSessionStateModule" />
    </modules>
  </system.webServer>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="BookSleeve" publicKeyToken="9056fda458ff61cf" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.2.0.5" newVersion="1.2.0.5" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>